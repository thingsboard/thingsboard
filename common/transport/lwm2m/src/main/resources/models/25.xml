<LWM2M xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.openmobilealliance.org/tech/profiles/LWM2M-v1_1.xsd">
	<Object ObjectType="MODefinition">
		<Name>LwM2M Gateway</Name>
		<Description1><![CDATA[This object is used by a LwM2M Gateway to maintain the devices connected to the gateway.]]></Description1>
		<ObjectID>25</ObjectID>
		<ObjectURN>urn:oma:lwm2m:oma:25</ObjectURN>
		<LWM2MVersion>1.2</LWM2MVersion>
		<ObjectVersion>1.0</ObjectVersion>
		<MultipleInstances>Multiple</MultipleInstances>
		<Mandatory>Optional</Mandatory>
		<Resources>
			<Item ID="0">
				<Name>Device ID</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[This resource identifies the device connected to the gateway.]]></Description>
			</Item>
			<Item ID="1">
				<Name>Prefix</Name>
				<Operations>RW</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[This resource defines what prefix has to be used for access to objects of this device.]]></Description>
			</Item>
			<Item ID="2">
				<Name>Routing Table Entry</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Objlnk</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[This resource links to the Object Instance in the LwM2M Routing Object.]]></Description>
			</Item>
		</Resources>
		<Description2 />
	</Object>
</LWM2M>
