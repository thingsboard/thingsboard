<LWM2M xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
  <Object ObjectType="MODefinition">
    <Name>Warning Light</Name>
    <Description1><![CDATA[This LWM2M Object includes Warning Light information.]]></Description1>
    <ObjectID>10353</ObjectID>
    <ObjectURN>urn:oma:lwm2m:x:10353</ObjectURN>
    <LWM2MVersion>1.0</LWM2MVersion>
    <ObjectVersion>1.0</ObjectVersion>
    <MultipleInstances>Single</MultipleInstances>
    <Mandatory>Optional</Mandatory>
    <Resources>
		<Item ID="1">
			<Name>Light Status</Name>
			<Operations>R</Operations>
			<MultipleInstances>Single</MultipleInstances>
			<Mandatory>Mandatory</Mandatory>
			<Type>Boolean</Type>
			<RangeEnumeration/>
			<Units/>
			<Description><![CDATA[The status of the light, 1:On, 0:Off.]]></Description>
		</Item>
		<Item ID="2">
			<Name>Light Warning</Name>
			<Operations>R</Operations>
			<MultipleInstances>Single</MultipleInstances>
			<Mandatory>Mandatory</Mandatory>
			<Type>Boolean</Type>
			<RangeEnumeration/>
			<Units/>
			<Description><![CDATA[If Light Warning is enabled or not, 1:Enabled, 0:Disabled.]]></Description>
		</Item>
		<Item ID="100">
			<Name>Light Control</Name>
			<Operations>E</Operations>
			<MultipleInstances>Single</MultipleInstances>
			<Mandatory>Mandatory</Mandatory>
			<Type/>
			<RangeEnumeration/>
			<Units/>
			<Description><![CDATA[Switch on/off the light: Parameter: 1:On, 0:Off]]></Description>
		</Item>
		<Item ID="101">
			<Name>Light Warning Control</Name>
			<Operations>E</Operations>
			<MultipleInstances>Single</MultipleInstances>
			<Mandatory>Mandatory</Mandatory>
			<Type/>
			<RangeEnumeration/>
			<Units/>
			<Description><![CDATA[Enable/Disable Light Warning: Parameter: 1:Enable, 0:Disable]]></Description>
		</Item>
    </Resources>
    <Description2 />
  </Object>
</LWM2M>
