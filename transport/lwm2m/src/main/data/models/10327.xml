<LWM2M xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
  <Object ObjectType="MODefinition">
    <Name>Compressor</Name>
    <Description1><![CDATA[This LWM2M Object includes the Compressor information.]]></Description1>
    <ObjectID>10327</ObjectID>
    <ObjectURN>urn:oma:lwm2m:x:10327</ObjectURN>
    <LWM2MVersion>1.0</LWM2MVersion>
    <ObjectVersion>1.0</ObjectVersion>
    <MultipleInstances>Multiple</MultipleInstances>
    <Mandatory>Optional</Mandatory>
    <Resources>
		<Item ID="1">
			<Name>Compressor Name</Name>
			<Operations>R</Operations>
			<MultipleInstances>Single</MultipleInstances>
			<Mandatory>Mandatory</Mandatory>
			<Type>String</Type>
			<RangeEnumeration/>
			<Units/>
			<Description><![CDATA[The name of the compressor.]]></Description>
		</Item>	
		
		<Item ID="50">
			<Name>Compressor Status</Name>
			<Operations>R</Operations>
			<MultipleInstances>Single</MultipleInstances>
			<Mandatory>Mandatory</Mandatory>
			<Type>Boolean</Type>
			<RangeEnumeration/>
			<Units/>
			<Description><![CDATA[The status of the compressor, 1:Working, 0:Stopped.]]></Description>
		</Item>
    </Resources>
    <Description2 />
  </Object>
</LWM2M>
