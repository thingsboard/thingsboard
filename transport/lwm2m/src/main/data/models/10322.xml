<LWM2M xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
  <Object ObjectType="MODefinition">
    <Name>CCU PM</Name>
    <Description1><![CDATA[This LWM2M Object includes CCU PM(counter and gauge).]]></Description1>
    <ObjectID>10322</ObjectID>
    <ObjectURN>urn:oma:lwm2m:x:10322</ObjectURN>
    <LWM2MVersion>1.0</LWM2MVersion>
    <ObjectVersion>1.0</ObjectVersion>
    <MultipleInstances>Multiple</MultipleInstances>
    <Mandatory>Optional</Mandatory>
    <Resources>
	  <Item ID="1">
        <Name>CPU Usage</Name>
        <Operations>R</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Optional</Mandatory>
        <Type>Integer</Type>
        <RangeEnumeration>0..100</RangeEnumeration>
        <Units>%</Units>
        <Description><![CDATA[The CPU usage of the CCU.]]></Description>
      </Item>
	  <Item ID="2">
        <Name>Max CPU Usage</Name>
        <Operations>R</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Optional</Mandatory>
        <Type>Integer</Type>
        <RangeEnumeration>0..100</RangeEnumeration>
        <Units>%</Units>
        <Description><![CDATA[Max CPU Usage of the CCU.]]></Description>
      </Item>
      <Item ID="3">
        <Name>Memory Usage</Name>
        <Operations>R</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Optional</Mandatory>
        <Type>Integer</Type>
        <RangeEnumeration>0..100</RangeEnumeration>
        <Units>%</Units>
        <Description><![CDATA[Memory Usage of the CCU.]]></Description>
      </Item>
      <Item ID="4">
        <Name>Storage Usage</Name>
        <Operations>R</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Optional</Mandatory>
        <Type>Integer</Type>
        <RangeEnumeration>0..100</RangeEnumeration>
        <Units>%</Units>
        <Description><![CDATA[Storage Usage of the CCU.]]></Description>
      </Item>
    </Resources>
    <Description2 />
  </Object>
</LWM2M>
